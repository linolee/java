/*
관계 연산자 :
	>	<	>=	<=	==	!-
*/
class Operator4{
	public static void main(String[] args) {
		int i = 11, j = 15, k = 11;
		
		System.out.println(i+"=="+k+"="+(i == k));  //ture
		System.out.println(i+"=="+j+"="+(i == j));	//false

		System.out.println(i+"=="+k+"="+(i != k));	//false
		System.out.println(i+"=="+j+"="+(i != j));	//ture

		System.out.println(i+"=="+k+"="+(i != k));	//false
		System.out.println(i+"=="+j+"="+(i != j));	//ture
		System.out.println(1==1 || 0==0);
	}//main
}//class
